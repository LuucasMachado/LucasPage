.Header
  background-image: url("../../images/Presentation/backgroudPresentation.jpg");
  width:100%;
  height: 70px;
  font-family: "Source Code Pro", sans-serif;
  background-attachment: fixed;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  .nav-header-sticky
    max-width: 1100px;
    margin:auto;
    box-sizing:border-box;
    display:flex;
    flex-direction: row;
  .nav-header-menu
    margin-left:auto;
    ul
      display: flex;
      flex-direction: row;
      li
        margin 0px,10px,0px,10px;
        list-style: none;
  .nav-header-menu-btn
    font-family: "Source Code Pro", sans-serif;
    background: transparent;
    text-decoration: none;
    color:white;
    font-size:18px;
    border:0px;
    cursor: pointer;
  .nav-header-logo
    color: white;
    margin:18px
    font-family: "Source Code Pro", sans-serif;
    font-size:22px;
