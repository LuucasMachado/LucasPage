.Header
  font-family: "Source Code Pro", sans-serif;
  margin-bottom: 20px
  .nav-header-sticky
    max-width: 1100px;
    margin:auto;
    box-sizing:border-box;
    display:flex;
    flex-direction: row;
  .nav-header-menu
    margin-left:auto;
    ul
      display: flex;
      flex-direction: row;
      li
        margin 10px;
        list-style: none;
  .nav-header-menu-btn
    font-family: 'Roboto Slab', serif;
    background: transparent;
    text-decoration: none;
    color:white;
    font-size:18px;
    border:0px;
    cursor: pointer;
  .nav-header-logo
    color: white;
    margin:18px
    font-family: 'Roboto Slab', serif;
    font-size:28px;
